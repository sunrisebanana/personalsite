{
  "name": "with-typescript",
  "version": "1.0.0",
  "homepage": "http://www.jaimeherzog.com",
  "scripts": {
    "predeploy": "npm run build && next export && touch out/.nojekyll && echo '_next' >> out/.nojekyll",
    "deploy": "gh-pages -d out -t true",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && next",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && next build",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && next start",
    "type-check": "tsc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/node": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "gh-pages": "^3.1.0",
    "leaflet": "^1.7.1",
    "leaflet-rastercoords": "^1.0.3",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.1.0",
    "react-lorem-ipsum": "^1.4.9",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "typescript": "^4.9.5",
    "underscore": "^1.12.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/leaflet": "^1.7.0",
    "@types/react": "^17.0.0",
    "@types/underscore": "^1.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4"
  },
  "license": "MIT"
}
